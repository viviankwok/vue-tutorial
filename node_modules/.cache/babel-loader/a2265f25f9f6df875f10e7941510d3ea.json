{"ast":null,"code":"import Header from \"./components/Header.vue\";\nimport Tasks from \"./components/Tasks.vue\";\nexport default {\n  name: \"App\",\n  components: {\n    Header,\n    Tasks\n  },\n\n  data() {\n    return {\n      tasks: []\n    };\n  },\n\n  methods: {\n    deleteTask(id) {\n      if (confirm(\"Are you sure?\")) {\n        this.tasks = this.tasks.filter(task => task.id !== id);\n      }\n    }\n\n  },\n\n  created() {\n    this.tasks = [{\n      id: 1,\n      text: \"Doctors appt\",\n      day: \"Mar 1\",\n      reminder: true\n    }, {\n      id: 2,\n      text: \"Meeting at school\",\n      day: \"Mar 3\",\n      reminder: true\n    }, {\n      id: 3,\n      text: \"Food shopping\",\n      day: \"Mar 3\",\n      reminder: false\n    }];\n  }\n\n};","map":{"version":3,"mappings":"AAQA,OAAOA,MAAP,MAAmB,yBAAnB;AACA,OAAOC,KAAP,MAAkB,wBAAlB;AAEA,eAAe;EACbC,IAAI,EAAE,KADO;EAEbC,UAAU,EAAE;IAAEH,MAAF;IAAUC;EAAV,CAFC;;EAGbG,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAE;IADF,CAAP;EAGD,CAPY;;EAQbC,OAAO,EAAE;IACPC,UAAU,CAACC,EAAD,EAAK;MACb,IAAIC,OAAO,CAAC,eAAD,CAAX,EAA8B;QAC5B,KAAKJ,KAAL,GAAa,KAAKA,KAAL,CAAWK,MAAX,CAAmBC,IAAD,IAAUA,IAAI,CAACH,EAAL,KAAYA,EAAxC,CAAb;MACF;IACD;;EALM,CARI;;EAebI,OAAO,GAAG;IACR,KAAKP,KAAL,GAAa,CACX;MACEG,EAAE,EAAE,CADN;MAEEK,IAAI,EAAE,cAFR;MAGEC,GAAG,EAAE,OAHP;MAIEC,QAAQ,EAAE;IAJZ,CADW,EAOX;MACEP,EAAE,EAAE,CADN;MAEEK,IAAI,EAAE,mBAFR;MAGEC,GAAG,EAAE,OAHP;MAIEC,QAAQ,EAAE;IAJZ,CAPW,EAaX;MACEP,EAAE,EAAE,CADN;MAEEK,IAAI,EAAE,eAFR;MAGEC,GAAG,EAAE,OAHP;MAIEC,QAAQ,EAAE;IAJZ,CAbW,CAAb;EAoBD;;AApCY,CAAf","names":["Header","Tasks","name","components","data","tasks","methods","deleteTask","id","confirm","filter","task","created","text","day","reminder"],"sourceRoot":"","sources":["/Users/vivian/Desktop/vue-crash-course/src/App.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n    <Header title=\"Task Tracker\" />\n    <Tasks @delete-task=\"deleteTask\" :tasks=\"tasks\" />\n  </div>\n</template>\n\n<script>\nimport Header from \"./components/Header.vue\";\nimport Tasks from \"./components/Tasks.vue\";\n\nexport default {\n  name: \"App\",\n  components: { Header, Tasks },\n  data() {\n    return {\n      tasks: [],\n    };\n  },\n  methods: {\n    deleteTask(id) {\n      if (confirm(\"Are you sure?\")) {\n        this.tasks = this.tasks.filter((task) => task.id !== id);\n      }\n    },\n  },\n  created() {\n    this.tasks = [\n      {\n        id: 1,\n        text: \"Doctors appt\",\n        day: \"Mar 1\",\n        reminder: true,\n      },\n      {\n        id: 2,\n        text: \"Meeting at school\",\n        day: \"Mar 3\",\n        reminder: true,\n      },\n      {\n        id: 3,\n        text: \"Food shopping\",\n        day: \"Mar 3\",\n        reminder: false,\n      },\n    ];\n  },\n};\n</script>\n\n<style>\n@import url(\"https://fonts.googleapis.com/css2?family=Poppins:wght@300;400&display=swap\");\n* {\n  box-sizing: border-box;\n  margin: 0;\n  padding: 0;\n}\nbody {\n  font-family: \"Poppins\", sans-serif;\n}\n.container {\n  max-width: 500px;\n  margin: 30px auto;\n  overflow: auto;\n  min-height: 300px;\n  border: 1px solid steelblue;\n  padding: 30px;\n  border-radius: 5px;\n}\n.btn {\n  display: inline-block;\n  background: #000;\n  color: #fff;\n  border: none;\n  padding: 10px 20px;\n  margin: 5px;\n  border-radius: 5px;\n  cursor: pointer;\n  text-decoration: none;\n  font-size: 15px;\n  font-family: inherit;\n}\n.btn:focus {\n  outline: none;\n}\n.btn:active {\n  transform: scale(0.98);\n}\n.btn-block {\n  display: block;\n  width: 100%;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}