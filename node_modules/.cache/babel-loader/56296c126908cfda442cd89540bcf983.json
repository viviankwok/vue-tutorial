{"ast":null,"code":"import Header from \"./components/Header.vue\";\nimport Tasks from \"./components/Tasks.vue\";\nimport AddTask from \"./components/AddTask.vue\";\nexport default {\n  name: \"App\",\n  components: {\n    Header,\n    Tasks,\n    AddTask\n  },\n\n  data() {\n    return {\n      tasks: []\n    };\n  },\n\n  methods: {\n    addTask(id) {\n      this.tasks = [...this.tasks, task];\n    },\n\n    deleteTask(id) {\n      if (confirm(\"Are you sure?\")) {\n        this.tasks = this.tasks.filter(task => task.id !== id);\n      }\n    },\n\n    toggleReminder(id) {\n      this.tasks = this.tasks.map(task => task.id === id ? { ...task,\n        reminder: !task.reminder\n      } : task);\n    }\n\n  },\n\n  created() {\n    this.tasks = [{\n      id: 1,\n      text: \"Doctors appt\",\n      day: \"Mar 1\",\n      reminder: true\n    }, {\n      id: 2,\n      text: \"Meeting at school\",\n      day: \"Mar 3\",\n      reminder: true\n    }, {\n      id: 3,\n      text: \"Food shopping\",\n      day: \"Mar 3\",\n      reminder: false\n    }];\n  }\n\n};","map":{"version":3,"mappings":"AAaA,OAAOA,MAAP,MAAmB,yBAAnB;AACA,OAAOC,KAAP,MAAkB,wBAAlB;AACA,OAAOC,OAAP,MAAoB,0BAApB;AAEA,eAAe;EACbC,IAAI,EAAE,KADO;EAEbC,UAAU,EAAE;IAAEJ,MAAF;IAAUC,KAAV;IAAiBC;EAAjB,CAFC;;EAGbG,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAE;IADF,CAAP;EAGD,CAPY;;EAQbC,OAAO,EAAE;IACPC,OAAO,CAACC,EAAD,EAAK;MACV,KAAKH,KAAL,GAAa,CAAC,GAAG,KAAKA,KAAT,EAAgBI,IAAhB,CAAb;IACD,CAHM;;IAIPC,UAAU,CAACF,EAAD,EAAK;MACb,IAAIG,OAAO,CAAC,eAAD,CAAX,EAA8B;QAC5B,KAAKN,KAAL,GAAa,KAAKA,KAAL,CAAWO,MAAX,CAAmBH,IAAD,IAAUA,IAAI,CAACD,EAAL,KAAYA,EAAxC,CAAb;MACF;IACD,CARM;;IASPK,cAAc,CAACL,EAAD,EAAK;MACjB,KAAKH,KAAL,GAAa,KAAKA,KAAL,CAAWS,GAAX,CAAgBL,IAAD,IAC1BA,IAAI,CAACD,EAAL,KAAYA,EAAZ,GAAiB,EAAE,GAAGC,IAAL;QAAWM,QAAQ,EAAE,CAACN,IAAI,CAACM;MAA3B,CAAjB,GAAyDN,IAD9C,CAAb;IAGD;;EAbM,CARI;;EAwBbO,OAAO,GAAG;IACR,KAAKX,KAAL,GAAa,CACX;MACEG,EAAE,EAAE,CADN;MAEES,IAAI,EAAE,cAFR;MAGEC,GAAG,EAAE,OAHP;MAIEH,QAAQ,EAAE;IAJZ,CADW,EAOX;MACEP,EAAE,EAAE,CADN;MAEES,IAAI,EAAE,mBAFR;MAGEC,GAAG,EAAE,OAHP;MAIEH,QAAQ,EAAE;IAJZ,CAPW,EAaX;MACEP,EAAE,EAAE,CADN;MAEES,IAAI,EAAE,eAFR;MAGEC,GAAG,EAAE,OAHP;MAIEH,QAAQ,EAAE;IAJZ,CAbW,CAAb;EAoBD;;AA7CY,CAAf","names":["Header","Tasks","AddTask","name","components","data","tasks","methods","addTask","id","task","deleteTask","confirm","filter","toggleReminder","map","reminder","created","text","day"],"sourceRoot":"","sources":["/Users/vivian/Desktop/vue-crash-course/src/App.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n    <Header title=\"Task Tracker\" />\n    <AddTask @add-task=\"addTask\" />\n    <Tasks\n      @toggle-reminder=\"toggleReminder\"\n      @delete-task=\"deleteTask\"\n      :tasks=\"tasks\"\n    />\n  </div>\n</template>\n\n<script>\nimport Header from \"./components/Header.vue\";\nimport Tasks from \"./components/Tasks.vue\";\nimport AddTask from \"./components/AddTask.vue\";\n\nexport default {\n  name: \"App\",\n  components: { Header, Tasks, AddTask },\n  data() {\n    return {\n      tasks: [],\n    };\n  },\n  methods: {\n    addTask(id) {\n      this.tasks = [...this.tasks, task];\n    },\n    deleteTask(id) {\n      if (confirm(\"Are you sure?\")) {\n        this.tasks = this.tasks.filter((task) => task.id !== id);\n      }\n    },\n    toggleReminder(id) {\n      this.tasks = this.tasks.map((task) =>\n        task.id === id ? { ...task, reminder: !task.reminder } : task\n      );\n    },\n  },\n\n  created() {\n    this.tasks = [\n      {\n        id: 1,\n        text: \"Doctors appt\",\n        day: \"Mar 1\",\n        reminder: true,\n      },\n      {\n        id: 2,\n        text: \"Meeting at school\",\n        day: \"Mar 3\",\n        reminder: true,\n      },\n      {\n        id: 3,\n        text: \"Food shopping\",\n        day: \"Mar 3\",\n        reminder: false,\n      },\n    ];\n  },\n};\n</script>\n\n<style>\n@import url(\"https://fonts.googleapis.com/css2?family=Poppins:wght@300;400&display=swap\");\n* {\n  box-sizing: border-box;\n  margin: 0;\n  padding: 0;\n}\nbody {\n  font-family: \"Poppins\", sans-serif;\n}\n.container {\n  max-width: 500px;\n  margin: 30px auto;\n  overflow: auto;\n  min-height: 300px;\n  border: 1px solid steelblue;\n  padding: 30px;\n  border-radius: 5px;\n}\n.btn {\n  display: inline-block;\n  background: #000;\n  color: #fff;\n  border: none;\n  padding: 10px 20px;\n  margin: 5px;\n  border-radius: 5px;\n  cursor: pointer;\n  text-decoration: none;\n  font-size: 15px;\n  font-family: inherit;\n}\n.btn:focus {\n  outline: none;\n}\n.btn:active {\n  transform: scale(0.98);\n}\n.btn-block {\n  display: block;\n  width: 100%;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}