{"ast":null,"code":"import Header from \"./components/Header.vue\";\nimport Tasks from \"./components/Tasks.vue\";\nimport AddTask from \"./components/AddTask.vue\";\nexport default {\n  name: \"App\",\n  components: {\n    Header,\n    Tasks,\n    AddTask\n  },\n\n  data() {\n    return {\n      tasks: [],\n      showAddTask: false\n    };\n  },\n\n  methods: {\n    toggleAddTask() {\n      this.showAddTask = !this.showAddTask;\n    },\n\n    addTask(task) {\n      this.tasks = [...this.tasks, task];\n    },\n\n    deleteTask(id) {\n      if (confirm(\"Are you sure?\")) {\n        this.tasks = this.tasks.filter(task => task.id !== id);\n      }\n    },\n\n    toggleReminder(id) {\n      this.tasks = this.tasks.map(task => task.id === id ? { ...task,\n        reminder: !task.reminder\n      } : task);\n    }\n\n  },\n\n  created() {\n    this.tasks = [{\n      id: 1,\n      text: \"Doctors appt\",\n      day: \"Mar 1\",\n      reminder: true\n    }, {\n      id: 2,\n      text: \"Meeting at school\",\n      day: \"Mar 3\",\n      reminder: true\n    }, {\n      id: 3,\n      text: \"Food shopping\",\n      day: \"Mar 3\",\n      reminder: false\n    }];\n  }\n\n};","map":{"version":3,"mappings":"AAeA,OAAOA,MAAP,MAAmB,yBAAnB;AACA,OAAOC,KAAP,MAAkB,wBAAlB;AACA,OAAOC,OAAP,MAAoB,0BAApB;AAEA,eAAe;EACbC,IAAI,EAAE,KADO;EAEbC,UAAU,EAAE;IAAEJ,MAAF;IAAUC,KAAV;IAAiBC;EAAjB,CAFC;;EAGbG,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAE,EADF;MAELC,WAAW,EAAE;IAFR,CAAP;EAID,CARY;;EAUbC,OAAO,EAAE;IACPC,aAAa,GAAG;MACd,KAAKF,WAAL,GAAmB,CAAC,KAAKA,WAAzB;IACD,CAHM;;IAIPG,OAAO,CAACC,IAAD,EAAO;MACZ,KAAKL,KAAL,GAAa,CAAC,GAAG,KAAKA,KAAT,EAAgBK,IAAhB,CAAb;IACD,CANM;;IAOPC,UAAU,CAACC,EAAD,EAAK;MACb,IAAIC,OAAO,CAAC,eAAD,CAAX,EAA8B;QAC5B,KAAKR,KAAL,GAAa,KAAKA,KAAL,CAAWS,MAAX,CAAmBJ,IAAD,IAAUA,IAAI,CAACE,EAAL,KAAYA,EAAxC,CAAb;MACF;IACD,CAXM;;IAYPG,cAAc,CAACH,EAAD,EAAK;MACjB,KAAKP,KAAL,GAAa,KAAKA,KAAL,CAAWW,GAAX,CAAgBN,IAAD,IAC1BA,IAAI,CAACE,EAAL,KAAYA,EAAZ,GAAiB,EAAE,GAAGF,IAAL;QAAWO,QAAQ,EAAE,CAACP,IAAI,CAACO;MAA3B,CAAjB,GAAyDP,IAD9C,CAAb;IAGD;;EAhBM,CAVI;;EA6BbQ,OAAO,GAAG;IACR,KAAKb,KAAL,GAAa,CACX;MACEO,EAAE,EAAE,CADN;MAEEO,IAAI,EAAE,cAFR;MAGEC,GAAG,EAAE,OAHP;MAIEH,QAAQ,EAAE;IAJZ,CADW,EAOX;MACEL,EAAE,EAAE,CADN;MAEEO,IAAI,EAAE,mBAFR;MAGEC,GAAG,EAAE,OAHP;MAIEH,QAAQ,EAAE;IAJZ,CAPW,EAaX;MACEL,EAAE,EAAE,CADN;MAEEO,IAAI,EAAE,eAFR;MAGEC,GAAG,EAAE,OAHP;MAIEH,QAAQ,EAAE;IAJZ,CAbW,CAAb;EAoBD;;AAlDY,CAAf","names":["Header","Tasks","AddTask","name","components","data","tasks","showAddTask","methods","toggleAddTask","addTask","task","deleteTask","id","confirm","filter","toggleReminder","map","reminder","created","text","day"],"sourceRoot":"","sources":["/Users/vivian/Desktop/vue-crash-course/src/App.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n    <Header @toggle-add-tasks=\"toggleAddTask\" title=\"Task Tracker\" />\n    <div v-show=\"showAddTask\">\n      <AddTask @add-task=\"addTask\" />\n    </div>\n    <Tasks\n      @toggle-reminder=\"toggleReminder\"\n      @delete-task=\"deleteTask\"\n      :tasks=\"tasks\"\n    />\n  </div>\n</template>\n\n<script>\nimport Header from \"./components/Header.vue\";\nimport Tasks from \"./components/Tasks.vue\";\nimport AddTask from \"./components/AddTask.vue\";\n\nexport default {\n  name: \"App\",\n  components: { Header, Tasks, AddTask },\n  data() {\n    return {\n      tasks: [],\n      showAddTask: false,\n    };\n  },\n\n  methods: {\n    toggleAddTask() {\n      this.showAddTask = !this.showAddTask;\n    },\n    addTask(task) {\n      this.tasks = [...this.tasks, task];\n    },\n    deleteTask(id) {\n      if (confirm(\"Are you sure?\")) {\n        this.tasks = this.tasks.filter((task) => task.id !== id);\n      }\n    },\n    toggleReminder(id) {\n      this.tasks = this.tasks.map((task) =>\n        task.id === id ? { ...task, reminder: !task.reminder } : task\n      );\n    },\n  },\n\n  created() {\n    this.tasks = [\n      {\n        id: 1,\n        text: \"Doctors appt\",\n        day: \"Mar 1\",\n        reminder: true,\n      },\n      {\n        id: 2,\n        text: \"Meeting at school\",\n        day: \"Mar 3\",\n        reminder: true,\n      },\n      {\n        id: 3,\n        text: \"Food shopping\",\n        day: \"Mar 3\",\n        reminder: false,\n      },\n    ];\n  },\n};\n</script>\n\n<style>\n@import url(\"https://fonts.googleapis.com/css2?family=Poppins:wght@300;400&display=swap\");\n* {\n  box-sizing: border-box;\n  margin: 0;\n  padding: 0;\n}\nbody {\n  font-family: \"Poppins\", sans-serif;\n}\n.container {\n  max-width: 500px;\n  margin: 30px auto;\n  overflow: auto;\n  min-height: 300px;\n  border: 1px solid steelblue;\n  padding: 30px;\n  border-radius: 5px;\n}\n.btn {\n  display: inline-block;\n  background: #000;\n  color: #fff;\n  border: none;\n  padding: 10px 20px;\n  margin: 5px;\n  border-radius: 5px;\n  cursor: pointer;\n  text-decoration: none;\n  font-size: 15px;\n  font-family: inherit;\n}\n.btn:focus {\n  outline: none;\n}\n.btn:active {\n  transform: scale(0.98);\n}\n.btn-block {\n  display: block;\n  width: 100%;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}